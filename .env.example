VITE_APP_NAME="${APP_NAME}"

# Metin2 Specific Settings
METIN2_SERVER_NAME="Your Server"
METIN2_SERVER_DESCRIPTION="Best Metin2 Server"
METIN2_EXP_RATE=10
METIN2_YANG_RATE=10
METIN2_DROP_RATE=10
METIN2_MAX_LEVEL=99

# External Links
METIN2_FORUM_URL=
METIN2_DISCORD_URL=
METIN2_FACEBOOK_URL=

# Security
SANCTUM_STATEFUL_DOMAINS=localhost,127.0.0.1,127.0.0.1:8000,::1
SESSION_SECURE_COOKIE=false